{
    "name": "hugofara/lwt-fork",
    "description": "Learn languages by reading, unofficial version.",
    "type": "library",
    "homepage": "https://github.com/HugoFara/lwt-fork",
    "license": "UNLICENSE",
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
